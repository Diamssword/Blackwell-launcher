{
  "name": "blackwell-launcher",
  "productName": "blackwell-launcher",
  "version": "1.0.0",
  "description": "Minecraft Custom launcher",
  "main": "src/index.js",
  "scripts": {
    "start": "electron src/index.js",
    "compile": "postcss ./src/index.css -o ./src/compiled.css -w | electron src/index.js",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "lint": "echo \"No linting configured\""
  },
  "build": {
    "appId": "OwlLauncher",
    "mac": {
      "category": "your.app.category.type"
    },
    "win": {
      "target": "portable"
    },
    "extraResources": [
      {
        "from": "src/mirrorurl.json",
        "to": "mirrorurl.json"
      }
    ]
  },
  "keywords": [],
  "author": {
    "name": "Stonksword",
    "email": "72971712+Stonksword@users.noreply.github.com"
  },
  "license": "MIT",
  "config": {
    "win": {
      "target": "portable "
    }
  },
  "dependencies": {
    "@material-design-icons/font": "^0.10.0",
    "bent": "^7.3.12",
    "chalk": "^4.1.2",
    "electron-squirrel-startup": "^1.0.0",
    "md5-file": "^5.0.0",
    "minecraft-launcher-core": "^3.16.8",
    "msmc": "^3.0.0-pre0",
    "node-fetch": "2.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.3.7",
    "electron-builder": "^22.13.1",
    "electron": "^15.1.0",
    "postcss": "^8.3.11",
    "postcss-cli": "^9.0.1",
    "tailwindcss": "^2.2.17"
  }
}
